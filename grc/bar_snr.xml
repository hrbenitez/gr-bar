<?xml version="1.0"?>
<block>
  <name>SNC</name>
  <key>bar_snr</key>
  <category>[bar]</category>
  <import>import bar</import>
  <make>bar.snr($threshold)</make>

  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <value>10.0</value>
    <type>float</type>
  </param>
  <sink>
    <name>in1</name>
    <type>float</type>
  </sink>
  <sink>
    <name>sync1</name>
    <type>float</type>
  </sink>
  <source>
    <name>s_re</name>
    <type>float</type>
  </source>
  <source>
    <name>n_re</name>
    <type>float</type>
  </source>
</block>
